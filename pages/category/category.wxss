page{
  height: 100%;
}
.cate{
  height: 100%;
}
.cate .cate_container{
  display: flex;
  height: calc( 100vh - 90rpx );
}
.cate .cate_container .left_menu{
  flex: 2;
  border-right: 2rpx solid #ccc;
}
.cate .cate_container .right_content{
  flex: 5;
} 
.cate .cate_container .right_content .goods_group{
} 
.cate .cate_container .right_content .goods_group .goods_title{
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
} 
.cate .cate_container .right_content .goods_group .goods_title .delimiter{
  color: #ccc;
  padding:0 10rpx;
} 
.cate .cate_container .right_content .goods_group .goods_title .title{
  
} 
.cate .cate_container .right_content .goods_group .goods_list{
  display: flex;
  flex-wrap: wrap;
} 
.cate .cate_container .right_content .goods_group .goods_list navigator{
  width: 33.33%;
  text-align: center;
} 
.cate .cate_container .right_content .goods_group .goods_list navigator image{
  width: 50%;
} 
.cate .cate_container .right_content .goods_group .goods_list navigator image .goods_name{
} 
.cate .cate_container .left_menu .menu_item{
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
}
.cate .cate_container .left_menu .active{
  color: var(--themeColor);
  border-left: 5rpx solid currentColor;
}